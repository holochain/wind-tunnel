<div class="mean-max">
    <span class="mean-max-mean">
        {{- template "scalar" (dict "value" .mean "unit" (index . "unit")) -}}
    </span>
    {{ if (ne .mean .max) }}
        <span class="mean-max-max">(max
            {{ template "scalar" (dict "value" .max "unit" (index . "unit")) -}})
        </span>
    {{ end }}
</div>
